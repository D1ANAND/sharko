{
  "name": "yellow-manifold-dapp",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/server.ts",
    "dev:frontend": "vite",
    "start": "node dist/server.js",
    "sync-bets": "tsx src/sync-bets.ts",
    "deploy:contract": "forge create --rpc-url $ARB_RPC --private-key $ORACLE_KEY --constructor-args $(cast wallet address $ORACLE_KEY) contracts/PredictionCustody.sol:PredictionCustody",
    "deploy:frontend": "vercel --prod"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.95.3",
    "@tanstack/react-query": "^5.90.20",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "viem": "^2.7.0",
    "wagmi": "^3.4.2",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.5.10",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-react": "^5.1.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3",
    "vite": "^7.3.1"
  }
}
